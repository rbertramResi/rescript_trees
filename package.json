{
  "name": "rescript-trees",
  "version": "0.0.1",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "set NODE_ENV=dev && npm-run-all --parallel start watch",
    "start": "serve",
    "re:build": "rescript",
    "re:clean": "rescript clean -with-deps",
    "re:start": "rescript build -w"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.3.1",
    "rescript": "*",
    "rescript-recoil": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.4.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.36.2",
    "rollup-plugin-import-css": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^11.3.2"
  }
}
